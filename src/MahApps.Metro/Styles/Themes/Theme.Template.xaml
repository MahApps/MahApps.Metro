    <Color x:Key="MahApps.Colors.SystemChromeDisabledHigh">{{MahApps.Colors.SystemChromeDisabledHigh}}</Color>
    <Color x:Key="MahApps.Colors.SystemChromeDisabledLow">{{MahApps.Colors.SystemChromeDisabledLow}}</Color>
    <Color x:Key="MahApps.Colors.SystemChromeHigh">{{MahApps.Colors.SystemChromeHigh}}</Color>
    <Color x:Key="MahApps.Colors.SystemChromeLow">{{MahApps.Colors.SystemChromeLow}}</Color>
    <Color x:Key="MahApps.Colors.SystemChromeMedium">{{MahApps.Colors.SystemChromeMedium}}</Color>
