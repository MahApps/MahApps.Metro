    <Color x:Key="MahApps.Colors.SystemBaseHigh">{{MahApps.Colors.SystemBaseHigh}}</Color>
    <Color x:Key="MahApps.Colors.SystemBaseLow">{{MahApps.Colors.SystemBaseLow}}</Color>
    <Color x:Key="MahApps.Colors.SystemBaseMedium">{{MahApps.Colors.SystemBaseMedium}}</Color>
