    <Color x:Key="MahApps.Colors.SystemChromeWhite">{{MahApps.Colors.SystemChromeWhite}}</Color>
    <Color x:Key="MahApps.Colors.SystemChromeGray">{{MahApps.Colors.SystemChromeGray}}</Color>
    <Color x:Key="MahApps.Colors.SystemListLow">{{MahApps.Colors.SystemListLow}}</Color>
