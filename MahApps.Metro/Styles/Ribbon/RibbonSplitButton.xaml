                    </Setter.Value>
                </Setter>
                <Setter TargetName="PART_Image" Property="FrameworkElement.Width">
                    <Setter.Value>
                        <s:Double>16</s:Double>
                    </Setter.Value>
                </Setter>
                <Setter TargetName="PART_Image" Property="FrameworkElement.Height">
                    <Setter.Value>
