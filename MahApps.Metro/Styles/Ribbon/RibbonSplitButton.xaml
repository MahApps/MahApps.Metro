            <DataTrigger Binding="{Binding Path=ControlSizeDefinition.IsLabelVisible, RelativeSource={RelativeSource Mode=Self}}" Value="False">
                <Setter TargetName="TwoLineText" Property="TextBlock.Text">
                    <Setter.Value>
